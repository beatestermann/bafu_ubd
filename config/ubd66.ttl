@prefix rr:     <http://www.w3.org/ns/r2rml#>.
@prefix rml:    <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:     <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#>.
@prefix oslo:   <http://purl.org/oslo/ns/localgov#>.
@prefix adms:   <http://www.w3.org/ns/adms#>.
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard:  <http://www.w3.org/2006/vcard/ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix adms:    <http://www.w3.org/ns/adms#>.
@prefix locn:    <http://www.w3.org/ns/locn#>.
@prefix schema: <http://schema.org/>.
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/>.
@prefix gont: <https://gont.ch/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qb: <http://purl.org/linked-data/cube#>.
@prefix bafu: <http://example.org/> .


<#Unit>
  rml:logicalSource [
    rml:source "input/ubd/66/UBD0066_unit.csv";
    rml:referenceFormulation ql:CSV
  ]; 

# TODO there is no ID for unit, might look ugly URIencoded

  rr:subjectMap [
    rr:template "http://environment.data.admin.ch/ubd/66/unit/{unit}";
    rr:class bafu:Unit ;
  ];

    rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "unit_de";
      rr:language "de" ;
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "unit_fr";
      rr:language "fr" ;
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "unit_it";
      rr:language "it" ;
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "unit_en";
      rr:language "en" ;
    ]
 ] ;

     rr:predicateObjectMap [
    rr:predicate rdfs:comment;
    rr:objectMap [
      rml:reference "unit_descr_de";
      rr:language "de" ;
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate rdfs:comment;
    rr:objectMap [
      rml:reference "unit_descr_fr";
      rr:language "fr" ;
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate rdfs:comment;
    rr:objectMap [
      rml:reference "unit_descr_it";
      rr:language "it" ;
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate rdfs:comment;
    rr:objectMap [
      rml:reference "unit_descr_en";
      rr:language "en" ;
    ]
 ] .

 ## TODO unit_remark is empty, not sure what this is supposed to be

<#SitePlot>
  rml:logicalSource [
    rml:source "input/ubd/66/UBD0066_site_plotID.csv";
    rml:referenceFormulation ql:CSV
  ]; 

# TODO there is no ID for unit, might look ugly URIencoded

  rr:subjectMap [
    rr:template "http://environment.data.admin.ch/ubd/66/siteplot/{site_plotID}";
    rr:class bafu:SitePlot ;
  ];

    rr:predicateObjectMap [
    rr:predicate bafu:site;
    rr:objectMap [
      rml:reference "site";
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate bafu:plot;
    rr:objectMap [
      rml:reference "plot";
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate bafu:municipality;
    rr:objectMap [
      rml:reference "community";
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate bafu:lv03_x_min;
    rr:objectMap [
      rml:reference "Xmin";
      rr:datatype xsd:float ;
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate bafu:lv03_y_min;
    rr:objectMap [
      rml:reference "Ymin";
      rr:datatype xsd:float ;
    ]
  ] ;
    rr:predicateObjectMap [
    rr:predicate bafu:lv03_x_max;
    rr:objectMap [
      rml:reference "Xmax";
      rr:datatype xsd:float ;
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate bafu:lv03_y_max;
    rr:objectMap [
      rml:reference "Ymax";
      rr:datatype xsd:float ;
    ]
  ] ;

    rr:predicateObjectMap [
    rr:predicate bafu:altitude;
    rr:objectMap [
      rml:reference "altitude" ;
      rr:datatype xsd:double ;
    ]
  ] .